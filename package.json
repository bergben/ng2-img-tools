{
    "name": "ng2-img-tools",
    "version": "1.1.0",
    "description": "Angular 2 toolkit to crop, resize or compress images in the browser.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bergben/ng2-img-tools.git"
    },
    "license": "MIT",
    "module": "./dist/ng2-img-tools.js",
    "main": "./dist/bundles/ng2-img-tools.umd.js",
    "typings": "./dist/ng2-img-tools.d.ts",
    "scripts": {
        "transpile": "ngc",
        "package": "rollup -c",
        "minify": "uglifyjs dist/bundles/ng2-img-tools.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng2-img-tools.umd.min.js",
        "build": "npm run transpile && npm run package && npm run minify"
    },
    "author": {
        "name": "Benedikt Berger",
        "email": "benedikt.berger@live.de"
    },
    "keywords": [
        "angular",
        "angular2",
        "img-tools",
        "thumbnails",
        "max",
        "size",
        "limit",
        "resize",
        "crop"
    ],
    "dependencies": {
        "ng2-img-max": "^2.1.6"
    },
    "peerDependencies": {
        "@angular/core": ">=2.0.0",
        "@angular/common": ">=2.0.0",
        "@angular/platform-browser": ">=2.0.0",
        "rxjs": ">=5.0.0-beta.12",
        "core-js": ">=2.4.1"
    },
    "devDependencies": {
        "@angular/common": ">=2.4.8",
        "@angular/compiler": ">=2.4.8",
        "@angular/compiler-cli": ">=2.4.8",
        "@angular/core": ">=2.4.8",
        "@angular/platform-browser": ">=2.4.8",
        "@angular/platform-browser-dynamic": ">=2.4.8",
        "@types/node": "^7.0.5",
        "core-js": ">2.4.1",
        "reflect-metadata": ">0.1.3",
        "rollup": ">0.41.4",
        "rxjs": ">5.1.1",
        "tslint": ">4.4.2",
        "typescript": ">2.1.6",
        "uglify-js": ">2.7.5",
        "zone.js": ">0.7.7"
    },
    "bugs": {
        "url": "https://github.com/bergben/ng2-img-tools/issues"
    },
    "homepage": "https://github.com/bergben/ng2-img-tools#readme"
}
